{
  "expo": {
    "name": "Road-Way-Weather-App",
    "slug": "Road-Way-Weather-App",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/dark-sky.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.theeban.Speak"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/mobile.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.theeban.Speak"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
